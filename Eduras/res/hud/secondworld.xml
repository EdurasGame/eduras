<?xml version="1.0" encoding="UTF-8"?>
<nifty xmlns="nifty.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="nifty.xsd nifty.xsd">
	<useStyles filename="nifty-default-styles.xml" />
	<useControls filename="nifty-default-controls.xml" />
	<screen id="stop">
		<layer id="mainlayer" childLayout="vertical" visibleToMouse="true">
			<panel backgroundColor="#003f" width="100%" height="100%"
				childLayout="center">
				<control type="label" id="helloLabel" font="aurulent-sans-16.fnt" color="#ffff"
					text="Bye world. middleclick to go to back" />
			</panel>
		</layer>
	</screen>
</nifty>
